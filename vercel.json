{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "app.py": {
      "runtime": "python3.12",
      "includeFiles": ["index.html", "favicon.ico"]
    }
  },
  "routes": [
    { "src": "^/favicon\\.ico$", "dest": "app.py" },
    { "src": "^/generate$", "dest": "app.py" },
    { "src": "^/(.*)$", "dest": "app.py" }
  ]
}
